{% extends 'base.html.twig' %}

{% block body %}
    <h1>Toutes les séries de la catégorie « {{ categoryName }} » : </h1>
    {% for program in programs %}
        <div>
            <h2>{{ loop.index }} / {{ program.title }}</h2>
            <p>{{ program.summary }}</p>
        </div>
    {% endfor %}
{% endblock %}